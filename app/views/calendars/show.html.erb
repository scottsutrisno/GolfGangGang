    <header>
      <nav class="nav">
        <ul>
      <% if user_signed_in? %>
        <li><%= link_to "Logout", destroy_user_session_path, :method => :delete %></li>
      <% else %>
        <li><%= link_to 'Sign In!', new_user_session_path, class: "signin" %></li>
      <% end %>
        <li><%= link_to "Calendar", calendars_path, class: "current" %></li>
        <li><%= link_to "Weather", root_path %></li>
        <li><%= link_to "My Groups", groups_path %></li>
        <li><%= link_to "Users", users_path %></li>
        </ul>
      </nav>
    </header>

<div class = "devforms2">
        <h1>Group: </h1>
  <table class="table2">
    <thead>
      <tr>
        <th scope="col">Event:</th>
        <th scope="col">Start-Time</th>
        <th scope="col">End-Time</th>
        <th scope="col">Groups</th>
      </tr>
    </thead>
    <tbody>

      <tr>
        <td><%= @calendar.name %></td>
        <td><%= @calendar.start_time.strftime("%I:%M  %p") %></td>
        <td><%= @calendar.end_time.strftime("%I:%M  %p") %></td>
        <td class="grp"><%= link_to "Edit", edit_calendar_path(@calendar) %></td>
        <td class="grp"><%= link_to 'Delete', @calendar, method: :delete, data: { confirm: 'Are you Sure?' } %></td>

      </tr>
    </tbody>
  </table>

<h1>Attending This Event:</h1>
<% @users.each do |user| %>
<p>Username: <%= user.username %>
Email: <%= user.email %>,</p>
<p>Name: <%= user.last_name %>, <%= user.first_name %></p>
<% end %>
</div>